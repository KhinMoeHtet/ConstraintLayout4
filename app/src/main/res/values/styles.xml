<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Text_title">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#040303</item>

    </style>

    <style name="txt_normal_style">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#EE000000</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="txt_column_style">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#EEFF7777</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="bank_info_txtStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#040303</item>

    </style>


</resources>